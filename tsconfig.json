{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@jsx/*": [
                "src/jsx/*"
            ],
            "@scss/*": [
                "src/scss/*"
            ],
            "@images/*": [
                "src/assets/images/*"
            ],
            "@svg/*": [
                "src/assets/svg/*"
            ],
            "@videos/*": [
                "src/assets/videos/*"
            ],
            "@components/*": [
                "src/jsx/components/*"
            ],
            "@contexts/*": [
                "src/jsx/contexts/*"
            ],
            "@data/*": [
                "src/jsx/data/*"
            ],
            "@pages/*": [
                "src/jsx/pages/*"
            ],
            "@services/*": [
                "src/jsx/services/*"
            ],
            "@abstracts/*": [
                "src/scss/abstracts/*"
            ],
            "@base/*": [
                "src/scss/base/*"
            ],
            "@scssComponents/*": [
                "src/scss/components/*"
            ],
            "@utilities/*": [
                "src/scss/utilities/*"
            ],
            "@utilities-override/*": [
                "src/scss/utilities-override/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}