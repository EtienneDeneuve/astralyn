{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@jsx/*": [
                "src/jsx/*"
            ],
            "@scss/*": [
                "src/scss/*"
            ],
            "@images/*": [
                "src/assets/images/*"
            ],
            "@components/*": [
                "src/jsx/components/*"
            ],
            "@contexts/*": [
                "src/jsx/contexts/*"
            ],
            "@data/*": [
                "src/jsx/data/*"
            ],
            "@pages/*": [
                "src/jsx/pages/*"
            ],
            "@utils/*": [
                "src/jsx/utils/*"
            ],
            "@abstracts/*": [
                "src/scss/abstracts/*"
            ],
            "@base/*": [
                "src/scss/base/*"
            ],
            "@scssComponents/*": [
                "src/scss/components/*"
            ],
            "@utilities/*": [
                "src/scss/utilities/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@data-grid/*": [
                "src/jsx/data/data-grid/*"
            ],
            "@charts/*": [
                "src/jsx/data/charts/*"
            ],
            "@calander/*": [
                "src/jsx/data/calander/*"
            ],
            "@map/*": [
                "src/jsx/data/map/*"
            ],
            "@kanban/*": [
                "src/jsx/data/kanban/*"
            ],
            "@editor/*": [
                "src/jsx/data/editor/*"
            ]
        }
    }
}